<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.studyHard.teamnova_android"
    android:compileSdkVersionCodename="1"
    android:versionName="1.0">


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_USER_DICTIONARY"/>
    <uses-permission android:name = "android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />





    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"   
        android:theme="@style/Theme.Teamnova_android"
        android:networkSecurityConfig="@xml/network_security_config">

        <activity
            android:name="com.studyHard.teamnova_android.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name="com.studyHard.teamnova_android.timer_stopwatch"></activity>

        <activity android:name="com.studyHard.teamnova_android.timer_preset"></activity>

        <activity android:name="com.studyHard.teamnova_android.timer_layout"></activity>

        <activity android:name="com.studyHard.teamnova_android.login"></activity>

        <activity android:name="com.studyHard.teamnova_android.login_signUp"></activity>

        <activity android:name="com.studyHard.teamnova_android.SettingActivity"></activity>

        <activity android:name="com.studyHard.teamnova_android.todo"></activity>

        <activity android:name="com.studyHard.teamnova_android.memo"></activity>

        <activity android:name="com.studyHard.teamnova_android.chart"></activity>

        <activity android:name="com.studyHard.teamnova_android.chart2"></activity>

        <activity android:name="com.studyHard.teamnova_android.유튜브API.Youtube_Main"></activity>

        <activity android:name="com.studyHard.teamnova_android.Papago.Papago"></activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.studyHard.teamnova_android.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths"></meta-data>
        </provider>

    </application>

</manifest>